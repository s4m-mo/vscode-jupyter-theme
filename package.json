{
    "name": "jupyter-theme",
    "displayName": "Jupyter Theme",
    "description": "A colour theme based around Jupyter and Kaggle.",
    "publisher": "s4m-mo",
    "repository": "https://github.com/s4m-mo/vscode-jupyter-theme",
    "icon": "resources/icon.png",
    "version": "0.1.3",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Themes",
        "Data Science",
        "Machine Learning",
        "Notebooks"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Jupyter Theme",
                "uiTheme": "vs",
                "path": "./themes/Jupyter Theme-color-theme.json"
            },
            {
                "label": "Jupyter Theme Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/Jupyter Theme Dark-color-theme.json"
            },
            {
                "label": "Jupyter Theme Light High Contrast",
                "uiTheme": "hc-light",
                "path": "./themes/High-Contrast-color-theme.json"
            },
            {
                "label": "Jupyter Theme Dark High Contrast",
                "uiTheme": "hc-black",
                "path": "./themes/High-Contrast-dark-color-theme.json"
            }
        ]
    }
}
